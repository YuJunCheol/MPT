package DB;

public class Insert extends DBConnection{
	
	public Insert() {
		
	}
	
	public void Insert(Data.food food) {
		//code,name,size,kcal,car,pro,fat
		String val = "'"+food.getFOOD_CD()+ "','"+food.getDESC_KOR()+ "','"+food.getSERVING_SIZE()+ "','"+food.getNUTR_CONT1()+"','"+food.getNUTR_CONT2()+"','"+food.getNUTR_CONT3()+"','"+food.getNUTR_CONT4()+"'";
		String sql = "INSERT INTO food VALUES ("+val+");";
		try {
			r = st.executeUpdate(sql);
		}catch(Exception e) {
			System.out.println("(!)[insert.User]: " + e.getMessage());
		}
	}
}
